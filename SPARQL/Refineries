#defaultView:Map
# Objective: find all Australian refineries and return their geolocation
# Source: Wikidata query, https://w.wiki/9dms

SELECT ?refinery ?refineryLabel ?geo WHERE {
  SERVICE wikibase:label { bd:serviceParam wikibase:language "[AUTO_LANGUAGE],en". }
  ?refinery wdt:P31 wd:Q1867977;
    wdt:P625 ?geo.
  ?refinery wdt:P17 wd:Q408 .
}

LIMIT 100
